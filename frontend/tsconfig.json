{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": "."
  },
  "paths": {
    "src/*": ["src/*"],
    "components/*": ["src/components/*"],
    "layouts/*": ["src/layouts/*"],
    "pages/*": ["src/pages/*"],
    "assets/*": ["src/assets/*"],
    "boot/*": ["src/boot/*"]
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
